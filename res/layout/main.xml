<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<TextView
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:text="@string/hello_world"
    />
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="发送Event事件" android:id="@+id/NewEventButton"></Button>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="设置用户信息" android:id="@+id/UserInfoButton"></Button>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="记录Exception" android:id="@+id/LogExceptionButton"></Button>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Crash" android:id="@+id/CrashButton"></Button>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="WebView" android:id="@+id/WebViewButton"></Button>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="退出" android:id="@+id/QuitButton"></Button>
</LinearLayout>
